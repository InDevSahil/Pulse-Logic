                },
                "10%": {
                    "opacity": "1"
                },
                "90%": {
                    "opacity": "1"
                },
                "100%": {
                    "top": "100%",
                    "opacity": "0"
                }
            },
            "glitchText": {
                "0%": {
                    "transform": "translate(0)"
                },
                "20%": {
                    "transform": "translate(-2px, 2px)"
                },
                "40%": {
                    "transform": "translate(-2px, -2px)"
                },
                "60%": {
                    "transform": "translate(2px, 2px)"
                },
                "80%": {
                    "transform": "translate(2px, -2px)"
                },
                "100%": {
                    "transform": "translate(0)"
                }
            },
            "fadeInUp": {
                "from": {
                    "opacity": "0",
                    "transform": "translateY(20px)"
                },
                "to": {
                    "opacity": "1",
                    "transform": "translateY(0)"
                }
            },
            "levelUpFloat": {
                "0%": {
                    "transform": "translateY(0) scale(1)",
                    "opacity": "0"
                },
                "20%": {
                    "opacity": "1",
                    "transform": "translateY(-10px) scale(1.1)"
                },
                "100%": {
                    "transform": "translateY(-40px) scale(1)",
                    "opacity": "0"
                }
            }
        },
